{
    "author": "KBV",
    "name": "mioviewer",
    "version": "1.3.0",
    "license": "LGPL-3.0",
    "description": "MIO Viewer zur Darstellung von MIOs in einem Webbrowser",
    "repository": "github:kassenaerztliche-bundesvereinigung/mioviewer",
    "bugs": {
        "url": "https://github.com/kassenaerztliche-bundesvereinigung/mioviewer/issues",
        "email": "support.mio@kbv.de"
    },
    "keywords": [
        "kbv",
        "mio",
        "viewer",
        "fhir"
    ],
    "scripts": {
        "test": "react-scripts test --all --verbose --watchAll=false",
        "test:watch": "react-scripts test --all --verbose",
        "test:coverage": "react-scripts test --all --verbose --coverage --watchAll=false --testResultsProcessor",
        "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache --verbose",
        "lint": "eslint src/**/*.{js,ts,tsx}",
        "postinstall": "npm unlink miotestdata && npm link miotestdata",
        "prettier": "prettier --write .",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!src/**/index.ts",
            "!src/**/*.test.{ts,tsx}",
            "!src/index.tsx",
            "!src/serviceWorker.ts"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 10,
                "functions": 10,
                "lines": 10,
                "statements": 10
            }
        },
        "coverageReporters": [
            "lcov",
            "text"
        ],
        "moduleNameMapper": {
            "^worker-loader!.+$": "<rootDir>/src/typings/EmptyModule"
        }
    },
    "jestSonar": {
        "reportPath": "reports",
        "reportFile": "test-report.xml",
        "indent": 4
    },
    "dependencies": {
        "@ionic/react": "^5.5.3",
        "@ionic/react-router": "^5.5.3",
        "@kbv/mioparser": "^1.3.0",
        "@types/luxon": "^1.26.3",
        "@types/node": "^14.14.22",
        "@types/pdfmake": "^0.1.16",
        "@types/react": "^16.14.2",
        "@types/react-dom": "^16.9.10",
        "@types/react-redux": "^7.1.16",
        "@types/react-router": "^5.1.11",
        "@types/react-router-dom": "^5.1.7",
        "@types/react-transition-group": "^4.4.0",
        "dompurify": "^2.2.8",
        "lodash.debounce": "^4.0.8",
        "lottie-web": "^5.7.6",
        "luxon": "^1.26.0",
        "moment": "^2.29.1",
        "pdfmake": "^0.1.70",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-feather": "^2.0.9",
        "react-json-view": "^1.21.1",
        "react-redux": "^7.2.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^3.4.4",
        "react-textfit": "^1.1.1",
        "react-transition-group": "^4.4.1"
    },
    "devDependencies": {
        "@ionic/react-test-utils": "0.0.3",
        "@testing-library/jest-dom": "^5.11.9",
        "@testing-library/react": "^10.4.8",
        "@types/dompurify": "^2.2.2",
        "@types/jest": "^26.0.20",
        "@types/lodash.debounce": "^4.0.6",
        "@typescript-eslint/eslint-plugin": "^3.8.0",
        "@typescript-eslint/parser": "^3.8.0",
        "comlink": "^4.3.0",
        "eslint": "^6.6.0",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.22.0",
        "jest": "24.9.0",
        "jest-canvas-mock": "^2.3.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.2.1",
        "react-animate-height": "^2.0.23",
        "react-fitty": "^1.0.0",
        "react-hot-loader": "^4.13.0",
        "typesafe-actions": "^5.1.0",
        "typescript": "^3.9.7",
        "worker-loader": "^3.0.7"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
